<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">

    <Document>
    <Style id="dyn_obstacle_style">
        <LineStyle>
            <color>7f00ffff</color>
            <width>4</width>
        </LineStyle>
        <PolyStyle>
            <color>7f00ff00</color>
        </PolyStyle>
        <Icon>
            <href>/static/dobs.png</href>
        </Icon>
    </Style>

    <Placemark>
        <name>Dynamic Obstacle ({{moving_obstacle_data}})</name>
        <styleUrl>#dyn_obstacle_style</styleUrl>
        <Point>
            <altitudeMode>relativeToGround</altitudeMode>
            <coordinates>
                {{ moving_obstacle_data }}
            </coordinates>
        </Point>
    </Placemark>
    </Document>

</kml>

